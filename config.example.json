{
  "LoopDelay": 600,
  "Cooldown": 864000,
  "EnableMultiFactorRequests": true,
  "Quorum": {
    "Enabled": false,
    "CheckDelayMultiplier": 1.25,
    "QuorumJitterLowerBound": 30,
    "QuorumJitterUpperBound": 60
  },
  "Triggers": [
    {
      "TriggerCriteria": {
        "TweetContains": [
          "Wing Attack Plan R"
        ],
        "RetweetsOver": 10000,
        "FavoritesOver": 1000
      },
      "Check": [
        "publictwitter"
      ],
      "Services": [
        {
          "Id": "publictwitter",
          "Actions": [
            "lock"
          ]
        },
        {
          "Id": "sinfultwitter",
          "Actions": [
            "scorch"
          ]
        },
        {
          "Id": "mywebsite",
          "Actions": [
            "update"
          ]
        }
      ],
      "NotifyServices": null
    }
  ],
  "Services": [
    {
      "Type": "twitterapi",
      "Id": "publictwitter",
      "CheckQuorum": true,
      "Options": {
        "ConsumerKey": "example",
        "ConsumerSecret": "example",
        "Token": "example",
        "TokenSecret": "example",
        "PostStatus": null
      }
    },
    {
      "Type": "twitterweb",
      "Id": "sinfultwitter",
      "CheckQuorum": true,
      "Options": {
        "Username": "mybadaccount",
        "Password": "example"
      }
    },
    {
      "Type": "cloudflare",
      "Id": "mywebsite",
      "CheckQuorum": true,
      "Options": {
        "Email": "example@example.com",
        "ApiKey": "example",
        "ZoneIds": [
          "1234"
        ],
        "RecordRegexes": [
          "A test",
          "SRV \\S* test record please ignore"
        ],
        "UpdateValues": {
          "A": "127.0.0.1",
          "AAAA": "::1",
          "CNAME": "example.com",
          "TXT": "example"
        }
      }
    }
  ],
  "NotifyServices": [
    {
      "Type": "pushover",
      "NotifyStartup": true,
      "NotifyMultiFactorRequests": true,
      "NotifyOnTrigger": true,
      "Options": {
        "Token": "123",
        "User": "321",
        "Device": null,
        "Priority": null,
        "Sound": null
      }
    }
  ]
}