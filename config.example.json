{
  "LoopDelay": 600,
  "Cooldown": 864000,
  "EnableMultiFactorRequests": true,
  "Quorum": {
    "Enabled": false,
    "CheckDelayMultiplier": 1.25,
    "QuorumJitterLowerBound": 30,
    "QuorumJitterUpperBound": 60
  },
  "Triggers": [
    {
      "TriggerCriteria": {
        "TweetContains": [
          "pen",
          "pineapple",
          "apple",
          "pen"
        ],
        "RetweetsOver": 10000,
        "FavoritesOver": 100
      },
      "Check": [
        "publictwitter"
      ],
      "Services": [
        {
          "Id": "publictwitter",
          "Actions": [
            "lock"
          ]
        },
        {
          "Id": "sinfultwitter",
          "Actions": [
            "scorch",
            "delete"
          ]
        }
      ],
      "NotifyServices": null
    }
  ],
  "Services": [
    {
      "Type": "twitterapi",
      "Id": "publictwitter",
      "CheckQuorum": true,
      "Options": {
        "ConsumerKey": "example",
        "ConsumerSecret": "example",
        "Token": "example",
        "TokenSecret": "example"
      }
    },
    {
      "Type": "twitterapi",
      "Id": "sinfultwitter",
      "CheckQuorum": true,
      "Options": {
        "ConsumerKey": "example",
        "ConsumerSecret": "example",
        "Token": "example",
        "TokenSecret": "example"
      }
    }
  ],
  "NotifyServices": [
    {
      "Type": "pushover",
      "NotifyStartup": true,
      "NotifyMultiFactorRequests": true,
      "NotifyOnTrigger": true,
      "Options": {
        "Token": "123",
        "User": "321",
        "Device": null,
        "Priority": null,
        "Sound": null
      }
    }
  ]
}